@model ProductsListViewModel

@foreach (var p in Model.Products)
{
    @Html.Partial("ProductSummary", p);
    //użycie widoku częściowego w pętli (ProductSummary.cshtml)
}
<!--Przekazanie stylów bootstrap i generowanie linków (infrastructure.cs)-->
<div page-model="@Model.PagingInfo" page-action="List"
     page-classes-enabled="true"
     page-class-normal="btn-secondary btn-lg"
     page-class-selected="btn-primary btn-lg"
     page-url-category="@Model.CurrentCategory"
     class="btn-group">
</div><!--page-url-category -> /Szachy/Strona1-->
